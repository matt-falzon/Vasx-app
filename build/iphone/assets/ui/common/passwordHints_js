function getPasswordHints(e,t,a){var r=Ti.Network.createHTTPClient();r.onerror=function(){Ti.API.error(this.status+" - "+this.statusText)};var i=[{SessionID:e,LanguageCode:t,VirtualNetwork:a}],n=generateXML("http://service.adminportal.vasx.com/","getPasswordHints",i);r.open("POST","http://192.168.20.3:12080/vx_adminportal/AdminPortal"),r.setRequestHeader("Content-Type","text/xml"),r.setRequestHeader("Content-Length",n.length),r.setRequestHeader("SOAPAction",'"getPasswordHints"'),r.send(n),r.onload=function(){var e=r.responseXML.documentElement.getElementsByTagName("getPasswordHintsResponse");if(e&&e.length>0){Ti.API.info(e.item(0).text);var t=Ti.XML.parseString(e.item(0).text),a=t.getElementsByTagName("QuestionCode1"),i=t.getElementsByTagName("AnswerCode1"),n=t.getElementsByTagName("QuestionCode2"),o=t.getElementsByTagName("AnswerCode2"),s=t.getElementsByTagName("QuestionCode3"),l=t.getElementsByTagName("AnswerCode3");firstQuestionTextField.setValue(a.item(0).text),firstAnswerTextArea.setValue(i.item(0).text),secondQuestionTextField.setValue(n.item(0).text),secondAnswerTextArea.setValue(o.item(0).text),thirdQuestionTextField.setValue(s.item(0).text),thirdAnswerTextArea.setValue(l.item(0).text)}}}function setPasswordHints(e,t,a,r,i,n,o,s,l){var c=Ti.Network.createHTTPClient();c.onerror=function(){Ti.API.error(this.status+" - "+this.statusText)};var d=[{SessionID:e,LanguageCode:t,QuestionCode1:a,Answer1:r,QuestionCode2:i,Answer2:n,QuestionCode3:o,Answer3:s,VirtualNetwork:l}],u=generateXML("http://service.adminportal.vasx.com/","setPasswordHints",d);c.open("POST","http://192.168.20.3:12080/vx_adminportal/AdminPortal"),c.setRequestHeader("Content-Type","text/xml"),c.setRequestHeader("Content-Length",u.length),c.setRequestHeader("SOAPAction",'"setPasswordHints"'),c.send(u),c.onload=function(){var e=c.responseXML.documentElement.getElementsByTagName("setPasswordHintsResponse");if(e&&e.length>0){Ti.API.info(e.item(0).text);var t=Ti.XML.parseString(e.item(0).text);alert(t.getElementsByTagName("Message").item(0).text)}}}var win=Ti.UI.currentWindow;Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),theme("red"),win.backgroundColor=Ti.App.backgroundColor,myId=win.myId,getPasswordHints(myId,"en",1);var firstView=Ti.UI.createView({layout:"vertical",backgroundColor:Ti.App.foregroundColor,top:"3%",height:"29.5%",left:"3%",right:"3%",borderRadius:10}),firstQuestionTextField=Ti.UI.createTextField({top:"5%",text:"loading",width:"90%",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),firstAnswerTextArea=Ti.UI.createTextArea({top:"18%",left:"3%",right:"3%",bottom:"18%",font:{fontSize:"15sp",fontWeight:"bold"},textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),secondView=Ti.UI.createView({layout:"vertical",backgroundColor:Ti.App.foregroundColor,top:"36%",height:"29%",left:"3%",right:"3%",borderRadius:10}),secondQuestionTextField=Ti.UI.createTextField({top:"5%",text:"loading",width:"90%",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),secondAnswerTextArea=Ti.UI.createTextArea({top:"18%",left:"3%",right:"3%",bottom:"18%",font:{fontSize:"15sp",fontWeight:"bold"},textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),thirdView=Ti.UI.createView({layout:"vertical",backgroundColor:Ti.App.foregroundColor,top:"68%",height:"29%",left:"3%",right:"3%",borderRadius:10}),thirdQuestionTextField=Ti.UI.createTextField({top:"5%",text:"loading",width:"90%",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),thirdAnswerTextArea=Ti.UI.createTextArea({top:"18%",left:"3%",right:"3%",bottom:"18%",font:{fontSize:"15sp",fontWeight:"bold"},textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),save=Ti.UI.createButton({systemButton:Ti.UI.iPhone.SystemButton.SAVE}),save=Ti.UI.createButton({systemButton:Ti.UI.iPhone.SystemButton.SAVE});save.addEventListener("click",function(){setPasswordHints(myId,"en",firstQuestionTextField.getValue(),firstAnswerTextArea.getValue(),secondQuestionTextField.getValue(),secondAnswerTextArea.getValue(),thirdQuestionTextField.getValue(),thirdAnswerTextArea.getValue(),1)}),win.add(firstView),win.add(secondView),win.add(thirdView),win.rightNavButton=save,firstView.add(firstQuestionTextField),firstView.add(firstAnswerTextArea),secondView.add(secondQuestionTextField),secondView.add(secondAnswerTextArea),thirdView.add(thirdQuestionTextField),thirdView.add(thirdAnswerTextArea);