function createMainWindow(e,t,a,r,i,n){Ti.App.tabGroup=Ti.UI.createTabGroup();var o=Ti.UI.createWindow({title:"Home",backgroundColor:"#fff",url:"ui/common/home.js",id:e,msisdn:n,firstName:t,lastName:a,sms:r}),s=Ti.UI.createTab({icon:"images/common/house.png",title:"Home",window:o}),l=Ti.UI.createWindow({title:"Usage",backgroundColor:"#fff",url:"ui/common/usage.js",id:e,firstName:t,lastName:a}),c=Ti.UI.createTab({icon:"images/common/pie-chart.png",title:"Usage",window:l}),d=Ti.UI.createWindow({title:"Bills",backgroundColor:"#fff",url:"ui/common/bills.js",id:e,firstName:t,lastName:a}),u=Ti.UI.createTab({icon:"images/common/no-cash.png",title:"Bills",window:d}),h=Ti.UI.createWindow({title:"Services",backgroundColor:"#fff",url:"ui/common/servicesMenu.js",id:e,firstName:t,lastName:a}),p=Ti.UI.createTab({icon:"images/common/KS_nav_ui.png",title:"Services",window:h});"pt"==i?(o.setTitle("Casa"),s.setTitle("Casa"),l.setTitle("Uso"),c.setTitle("Uso"),d.setTitle("notas"),u.setTitle("notas"),h.setTitle("serviços"),p.setTitle("serviços")):"fr"==i?(o.setTitle("maison"),s.setTitle("maison"),l.setTitle("Utilisation"),c.setTitle("utilisation"),d.setTitle("Factures"),u.setTitle("Factures")):i="en",Ti.App.tabGroup.addTab(s),Ti.App.tabGroup.addTab(c),Ti.App.tabGroup.addTab(u),Ti.App.tabGroup.addTab(p),Ti.App.tabGroup.open({transition:Ti.UI.iPhone.AnimationStyle.FLIP_FROM_RIGHT})}exports.createMainWindow=createMainWindow;