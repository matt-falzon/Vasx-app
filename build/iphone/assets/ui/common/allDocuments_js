var win=Ti.UI.currentWindow;Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),theme("green"),win.backgroundColor=Ti.App.backgroundColor,win.addEventListener("focus",function(){if("en"==Ti.App.Language)var e=[{hasDetail:!0,title:"View aging data",url:"agingData.js"},{hasDetail:!0,title:"View unbilled calls",url:"unbilledCalls.js"},{hasDetail:!0,title:"View call details",url:"callDetails.js"},{hasDetail:!0,title:"Document List",url:"documentList.js"},{hasDetail:!0,title:"On net bundle",url:"onNetBundle.js"},{hasDetail:!0,title:"Itemised Billing",url:"itemisedBilling.js"},{hasDetail:!0,title:"Balance Enquiry",url:"balance.js"}];else if("fr"==Ti.App.Language)var e=[{hasDetail:!0,title:"Afficher vieillissement données",url:"agingData.js"},{hasDetail:!0,title:"Voir non facturés appels",url:"unbilledCalls.js"},{hasDetail:!0,title:"obtenir les détails des appels",url:"callDetails.js"},{hasDetail:!0,title:"Liste des documents",url:"documentList.js"}];else var e=[{hasDetail:!0,title:"Ver envelhecimento dados",url:"agingData.js"},{hasDetail:!0,title:"Ver não faturada chamadas",url:"unbilledCalls.js"},{hasDetail:!0,title:"ver os detalhes da chamada",url:"callDetails.js"},{hasDetail:!0,title:"lista de documento",url:"documentList.js"}];table.setData(e)});var table=Ti.UI.createTableView({backgroundColor:"transparent"}),mainView=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,top:"3%",bottom:"3%",right:"3%",left:"3%",borderRadius:10});table.addEventListener("click",function(e){if(e.rowData.url)var t=Ti.UI.createWindow({backgroundColor:"#fff",title:e.rowData.title,url:e.rowData.url,id:win.id});Ti.UI.currentTab.open(t)}),win.add(mainView),mainView.add(table);