var win=Ti.UI.currentWindow;Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),theme("red"),win.backgroundColor=Ti.App.backgroundColor,myId=win.id;for(var data=[],labels=["Free SMS","Please Call Me","Recharge","FriendlyNames","Bill Limit","Handset Upgrade","Password","Display Names","opps"],x=0;3>x;x++)for(var c=0;labels.length>c;c++){var item=Ti.UI.createDashboardItem({image:"/images/dashboard/"+labels[c]+"_off.png",selectedImage:"/images/dashboard/"+labels[c]+"_on.png",label:labels[c]});data.push(item)}var dashboard=Ti.UI.createDashboardView({data:data,canDelete:!1});win.add(dashboard),dashboard.addEventListener("click",function(e){if("Free SMS"==e.item.label){var t=Ti.UI.createWindow({backgroundColor:"#fff",title:"Free SMS",url:"freeSMS.js",id:myId});Ti.UI.currentTab.open(t)}if("Please Call Me"==e.item.label){var t=Ti.UI.createWindow({backgroundColor:"#fff",title:"Please Call Me",url:"pleaseCallMe.js"});Ti.UI.currentTab.open(t)}if("Recharge"==e.item.label){var t=Ti.UI.createWindow({backgroundColor:"#fff",title:"Voucher Recharge",url:"voucherRecharge.js",id:myId,lang:Ti.App.Language});Ti.UI.currentTab.open(t)}if("FriendlyNames"==e.item.label){var t=Ti.UI.createWindow({backgroundColor:"#fff",title:"Friendly Names",url:"friendlyNames.js",msisdn:Ti.App.msisdn});Ti.UI.currentTab.open(t)}if("Bill Limit"==e.item.label){var t=Ti.UI.createWindow({backgroundColor:"#fff",title:"Bill Limit",url:"billLimit.js"});Ti.UI.currentTab.open(t)}if("Password"==e.item.label){var t=Ti.UI.createWindow({backgroundColor:"#fff",title:"Password Hints",url:"passwordHints.js",myId:myId});Ti.UI.currentTab.open(t)}if("Display Names"==e.item.label){var t=Ti.UI.createWindow({backgroundColor:"#fff",title:"Display Names",url:"displayNames.js",myId:myId});Ti.UI.currentTab.open(t)}});var cancel=Ti.UI.createButton({systemButton:Ti.UI.iPhone.SystemButton.DONE});cancel.addEventListener("click",function(){dashboard.stopEditing(),win.rightNavButton=null}),dashboard.addEventListener("edit",function(){win.rightNavButton=cancel});