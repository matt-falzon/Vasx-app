var win=Ti.UI.currentWindow,myId=win.id;win.backgroundColor="#1BA5E0";var myTabGroup=Ti.UI.currentWindow.tabGroup,tableInfo=[{hasDetail:!0,title:"Free SMS",url:"freeSMS.js"},{hasDetail:!0,title:"Please call me",url:"pleaseCallMe.js"},{hasDetail:!0,title:"Voucher recharge",url:"voucherRecharge.js"},{hasDetail:!0,title:"Friendly Names",url:"friendlyNames.js"},{hasDetail:!0,title:"Bill Limit",url:"billLimit.js"},{hasDetail:!0,title:"Services Menu",url:"servicesMenu.js"}],logout=Ti.UI.createButton({title:"Log out",style:Ti.UI.iPhone.SystemButtonStyle.BORDERED}),table=Ti.UI.createTableView({data:tableInfo,backgroundColor:"transparent"}),theView=Ti.UI.createView({backgroundColor:"#E3E8E8",height:347,left:10,right:10,borderRadius:10,top:10});logout.addEventListener("click",function(){Ti.App.tabGroup.removeTab(Ti.UI.currentTab),Ti.App.tabGroup.close({transition:Ti.UI.iPhone.AnimationStyle.NONE})}),table.addEventListener("click",function(e){if(e.rowData.url)var t=Ti.UI.createWindow({backgroundColor:"#fff",title:e.rowData.title,url:e.rowData.url});Ti.UI.currentTab.open(t),Ti.App.theMessage=""}),theView.add(table),win.add(theView),win.setLeftNavButton(logout);