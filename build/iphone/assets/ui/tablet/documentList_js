function open(){function e(e,t,a,i){var l=Ti.Network.createHTTPClient();l.onerror=function(){Ti.API.error(this.status+" - "+this.statusText)};var c=[{SessionID:e,LanguageCode:t,ForMSISDN:a,VirtualNetwork:i}],d=generateXML("http://service.adminportal.vasx.com/","getDocumentList",c);l.open("POST","http://192.168.20.3:12080/vx_adminportal/AdminPortal"),l.setRequestHeader("Content-Type","text/xml"),l.setRequestHeader("Content-Length",d.length),l.setRequestHeader("SOAPAction",'"getDocumentList"'),l.send(d),l.onload=function(){var e=l.responseXML.documentElement.getElementsByTagName("getDocumentListResponse");if(e&&e.length>0){Ti.API.info(e.item(0).text);var t=Ti.XML.parseString(e.item(0).text);Ti.API.info(t.getElementsByTagName("Message").item(0).text);var a=[{title:t.getElementsByTagName("Message").item(0).text}];n.setData(a),s.add(n),r.add(s),o.setVisible(!1)}}}function t(){r.open()}function a(){r.close()}var r=Ti.UI.createWindow({backgroundColor:"pink",left:"15%"});swipeWindow(r),closeMenu(r),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),theme("green"),r.backgroundColor=Ti.App.backgroundColor;var i;i="iPhone OS"===Ti.Platform.name?Ti.UI.iPhone.ActivityIndicatorStyle.DARK:Ti.UI.ActivityIndicatorStyle.DARK;var o=Ti.UI.createActivityIndicator({color:Ti.App.foregroundColor,font:{fontFamily:"Helvetica Neue",fontSize:26,fontWeight:"bold"},message:"Loading...",style:i,height:Ti.UI.SIZE,width:Ti.UI.SIZE,visible:!1});r.addEventListener("focus",function(){o.setVisible(!0)});var n=Ti.UI.createTableView({backgroundColor:"transparent"});e(Ti.App.Session,Ti.App.Language,Ti.App.msisdn,1);var s=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,top:"3%",bottom:"3%",right:"3%",left:"3%",borderRadius:10});return n.addEventListener("click",function(e){if(e.rowData.url)var t=Ti.UI.createWindow({backgroundColor:"#fff",title:e.rowData.title,url:e.rowData.url,id:r.id});Ti.UI.currentTab.open(t)