function passwordHints(e){function t(e,t,a){var r=Ti.Network.createHTTPClient();r.onerror=function(){Ti.API.error(this.status+" - "+this.statusText)};var i=[{SessionID:e,LanguageCode:t,VirtualNetwork:a}],n=generateXML("http://service.adminportal.vasx.com/","getPasswordHints",i);r.open("POST","http://192.168.20.3:12080/vx_adminportal/AdminPortal"),r.setRequestHeader("Content-Type","text/xml"),r.setRequestHeader("Content-Length",n.length),r.setRequestHeader("SOAPAction",'"getPasswordHints"'),r.send(n),r.onload=function(){var e=r.responseXML.documentElement.getElementsByTagName("getPasswordHintsResponse");if(e&&e.length>0){Ti.API.info(e.item(0).text);var t=Ti.XML.parseString(e.item(0).text),a=t.getElementsByTagName("QuestionCode1"),i=t.getElementsByTagName("AnswerCode1"),n=t.getElementsByTagName("QuestionCode2"),l=t.getElementsByTagName("AnswerCode2"),d=t.getElementsByTagName("QuestionCode3"),h=t.getElementsByTagName("AnswerCode3");o.setValue(a.item(0).text),s.setValue(i.item(0).text),c.setValue(n.item(0).text),u.setValue(l.item(0).text),p.setValue(d.item(0).text),m.setValue(h.item(0).text)}}}function a(){var e=[o.getValue(),s.getValue(),c.getValue(),secondQuestionTextArea.getValue(),p.getValue(),thirdQuestionTextArea.getValue()];return e}var r=Ti.UI.createView({backgroundColor:"pink",left:"15%",top:0});swipeWindow(r),closeMenu(r),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),theme("red"),r.backgroundColor=Ti.App.backgroundColor,t(Ti.App.Session,"en",1);var i=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,height:"29.5%",borderRadius:10,top:0}),o=Ti.UI.createTextArea({top:"20%",text:"loading",width:"45%",left:"3%",height:"60%",font:{fontSize:"30sp"}}),n=Ti.UI.createLabel({text:":",font:{fontSize:"30sp"},top:"25%"}),s=Ti.UI.createTextArea({top:"20%",right:"3%",width:"45%",height:"60%",font:{fontSize:"30sp"}}),l=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,to