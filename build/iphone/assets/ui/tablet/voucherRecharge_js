function open(){function e(e,t,a,r){var i=Ti.Network.createHTTPClient();i.onerror=function(){Ti.API.error(this.status+" - "+this.statusText)};var o=[{SessionID:e,LanguageCode:t,PinNumber:a,VirtualNetwork:r}],n=generateXML("http://service.adminportal.vasx.com/","getRecharge",o);i.open("POST","http://192.168.20.3:12080/vx_adminportal/AdminPortal"),i.setRequestHeader("Content-Type","text/xml"),i.setRequestHeader("Content-Length",n.length),i.setRequestHeader("SOAPAction",'"getRecharge"'),i.send(n),i.onload=function(){var e=i.responseXML.documentElement.getElementsByTagName("getRechargeResponse");if(e&&e.length>0){var t=Ti.XML.parseString(e.item(0).text);alert(t.getElementsByTagName("Message").item(0).text)}}}function t(){r.open()}function a(){r.close()}var r=Ti.UI.createWindow({backgroundColor:"pink",left:"15%"});swipeWindow(r),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),theme("red"),r.backgroundColor=Ti.App.backgroundColor,closeMenu(r);var i=Ti.UI.createView({layout:"vertical",backgroundColor:Ti.App.foregroundColor,top:"3%",height:"20%",left:"3%",right:"3%",borderRadius:10}),o=Ti.UI.createTextField({top:"10%",bottom:"10%",width:"90%",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER,hintText:"Enter your recharge code",backgroundColor:"white",font:{fontSize:"24sp",fontWeight:"bold"},color:Ti.App.backgroundColor,autocorrect:!1}),n=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,height:"18%",left:"3%",right:"3%",top:"26%",borderRadius:10}),s=Ti.UI.createButton({title:"Update",top:"20%"});return s.addEventListener("click",function(){e(r.id,Ti.App.Language,o.getValue(),1)}),r.add(i),r.add(n),n.add(s),i.add(o),r.openWin=t,r.close=a,r}exports.open=open;