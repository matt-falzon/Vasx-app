function open(){function e(){a.open()}function t(){a.close()}var a=Ti.UI.createWindow({backgroundColor:"pink",left:"15%"});swipeWindow(a),closeMenu(a),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),theme("red"),a.backgroundColor=Ti.App.backgroundColor;var r=Ti.UI.createView({height:Ti.UI.SIZE,layout:"vertical",backgroundColor:Ti.App.backgroundColor,top:"3%"}),i=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,height:"51.5%",left:"3%",right:"3%",borderRadius:10}),o=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,top:"5%",left:"3%",right:"3%",bottom:"5%",borderRadius:10}),n=Ti.UI.createTextField({hintText:"Enter phone number",color:"black",top:"13%",width:"80%",height:"30%",font:{fontSize:24,fontWeight:"bold"},backgroundColor:"white",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),s=Ti.UI.createButton({title:"Contacts",top:"65%",left:"10%",width:"35%",bottom:"10%"}),l=Ti.UI.createButton({title:"Call Me",top:"65%",right:"10%",width:"35%",bottom:"10%"}),c=Ti.UI.createTableView({data:getContacts(),backgroundColor:"transparent",separatorColor:Ti.App.backgroundColor});return n.addEventListener("click",function(){n.blur(),n.keyboardType=Ti.UI.KEYBOARD_PHONE_PAD,n.focus()}),s.addEventListener("click",function(){n.blur()}),c.addEventListener("click",function(e){e.index,e.section,e.row,e.rowData;var t=e.rowData.msisdn;getContacts(),Ti.API.info(t),n.setValue(t)}),l.addEventListener("click",function(){""==n.getValue()?alert("Select a phone number first"):alert("Message sent!")}),i.add(n),a.add(r),r.add(i),r.add(o),i.add(l),i.add(s),o.add(c),a.openWin=e,a.close=t,a}exports.open=open;