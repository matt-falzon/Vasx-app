function agingData(e){function t(t,a,o){var n=Ti.Network.createHTTPClient();n.onerror=function(){Ti.API.error(this.status+" - "+this.statusText)};var s=[{SessionID:t,LanguageCode:a,VirtualNetwork:o}],l=generateXML("http://service.adminportal.vasx.com/","getAgingData",s);n.open("POST","http://192.168.20.3:12080/vx_adminportal/AdminPortal"),n.setRequestHeader("Content-Type","text/xml"),n.setRequestHeader("Content-Length",l.length),n.setRequestHeader("SOAPAction",'"getAgingData"'),n.send(l),n.onload=function(){var t=n.responseXML.documentElement.getElementsByTagName("getAgingDataResponse");if(Ti.API.info(t.item(0).text),t&&t.length>0){var a=[],o=[],s=Ti.XML.parseString(t.item(0).text);s.getElementsByTagName("Message");var l=s.getElementsByTagName("CurrentBalance"),c=s.getElementsByTagName("Outstanding30Days"),d=s.getElementsByTagName("Outstanding60Days"),u=s.getElementsByTagName("Outstanding90Days"),h=s.getElementsByTagName("Outstanding120Days"),p=s.getElementsByTagName("UnbilledUsage"),g=s.getElementsByTagName("UnbilledSubs");a.push(l.item(0).text),o.push("Current Balance: "),a.push(c.item(0).text),o.push("Outstanding 30 days: "),a.push(d.item(0).text),o.push("Outstanding 60 days: "),a.push(u.item(0).text),o.push("Outstanding 90 days: "),a.push(h.item(0).text),o.push("Outstanding 120 days: "),a.push(p.item(0).text),o.push("Unbilled Usage: "),a.push(g.item(0).text),o.push("Unbilled Subscribers: ");for(var m=[],f=0;a.length>f;f++){var T=Ti.UI.createTableViewRow({selectedBackgroundColor:"transparent",height:40}),v=Ti.UI.createLabel({text:o[f],color:"black",font:{fontSize:"16pt",fontWeight:"bold"},left:"2%",right:"2%"}),b=Ti.UI.createLabel({text:"$"+a[f],font:{fontSize:"19pt",fontWeight:"bold"},right:"2%",color:Ti.App.backgroundColor});T.add(v),T.add(b),m.push(T)}var y=Ti.UI.createTableView({backgroundColor:"transparent",data:m,separatorColor:Ti.App.backgroundColor});r.add(y),e.add(r),i.setVisible(!1)}}}Ti.UI.currentWindow,Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),theme("green");var a,r=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,top:"3%",bottom:"3%",right:"3%",left:"3%",borderRadius:10});a="iPhone OS"===Ti.Platform.name?Ti.UI.iPhone.ActivityIndicatorStyle.DARK:Ti.UI.ActivityIndicatorStyle.DARK;var i=Ti.UI.createActivityIndicator({color:Ti.App.foregroundColor,font:{fontFamily:"Helvetica Neue",fontSize:26,fontWeight:"bold"},message:"Loading...",style:a,height:Ti.UI.SIZE,width:Ti.UI.SIZE,visible:!1});i.setVisible(!0),t(Ti.App.Session,Ti.App.Language,1),e.add(i)}