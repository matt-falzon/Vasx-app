function main(e,t,a,r,i,o){Ti.App.Session=e,Ti.App.freeSMS=a,Ti.App.msisdn=t,Ti.App.firstName=r;var n,s,t=t,r=r,l=i;Ti.App.Language=o,Ti.API.info("free SMS: "+Ti.App.freeSMS),Ti.App.mainWin=Ti.UI.createWindow({backgroundColor:"black",opacity:0});var c=Ti.UI.createView({left:0,width:"15%",bottom:0,backgroundColor:"#BEBBBF"});if("en"==o)var d=[{title:"Home",url:"ui/tablet/home"},{title:"Bills",url:"ui/tablet/bills"},{title:"Free SMS",url:"ui/tablet/freeSMS"},{title:"Please Call Me",url:"ui/tablet/pleaseCallMe"},{title:"My details",url:"ui/tablet/viewDetails"},{title:"Friendly Names",url:"ui/tablet/friendlyNames"},{title:"Documents",url:"ui/tablet/documentList"},{title:"Call Details",url:"ui/tablet/callDetails"},{title:"blank"},{title:"blank"},{title:"blank"}];else if("pt"==o)var d=[{title:"Casa",url:"ui/tablet/home"},{title:"Notas",url:"ui/tablet/bills"},{title:"sms grátis",url:"ui/tablet/freeSMS"},{title:"Por favor, me chame",url:"ui/tablet/pleaseCallMe"},{title:"Meus dados",url:"ui/tablet/viewDetails"},{title:"Nomes amigáveis",url:"ui/tablet/friendlyNames"},{title:"em branco"},{title:"em branco"},{title:"em branco"},{title:"em branco"},{title:"em branco"}];else if("fr"==o)var d=[{title:"Home",url:"ui/tablet/home"},{title:"Notas",url:"ui/tablet/bills"},{title:"Free SMS",url:"ui/tablet/freeSMS"},{title:"Please Call Me",url:"ui/tablet/pleaseCallMe"},{title:"My details",url:"ui/tablet/viewDetails"},{title:"Nomes amigáveis",url:"ui/tablet/friendlyNames"},{title:"blank"},{title:"blank"},{title:"blank"},{title:"blank"},{title:"blank"},{title:"blank"}];for(var u=[],h=["home","bills","freeSMS","pleaseCallMe","detail","FriendlyNames","documents","callDetails"],p=0;d.length>p;p++){var g=Ti.UI.createTableViewRow({selectedBackgroundColor:"transparent",height:"11%",url:d[p].url,selectedBackgroundColor:Ti.App.foregroundColor}),m=Ti.UI.createLabel({text:d[p].title,color:"white",font:{fontSize:"16pt",fontWeight:"bold"},textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER,bottom:"5%"}),f=Ti.UI.createImageView({image:"images/tablet/sidemenu/"+h[p]+".png",top:"10%",left:"20%",right:"20%",bottom:"20%"});g.add(m),g.add(f),u.push(g)}var T=Ti.UI.createTableView({data:u,rowHeight:"20%",backgroundColor:"transparent",fontColor:"white",allowsSelection:!0,showVerticalScrollIndicator:!1});c.add(T),Ti.App.mainWin.add(c);var n=require("ui/tablet/home");Ti.App.mainWin.open();var s=new n.open(0,Ti.App.Session,"en",Ti.App.msisdn,Ti.App.firstName);s.openWin(),T.addEventListener("click",function(e){"ui/tablet/home"==e.rowData.url?(s.close(),n=require(e.rowData.url),s=new n.open(15,Ti.App.Session,"en",Ti.App.msisdn,Ti.App.firstName,l,Ti.App.freeSMS),s.openWin()):"ui/tablet/freeSMS"==e.rowData.url?(s.close(),n=require(e.rowData.url),s=new n.open(Ti.App.Session),s.openWin()):"ui/tablet/bills"==e.rowData.url?(s.close(),n=require(e.rowData.url),s=new n.open(Ti.App.Session,Ti.App.freeSMS),s.openWin()):(s.close(),n=require(e.rowData.url),s=new n.open,s.openWin())})}exports.main=main;