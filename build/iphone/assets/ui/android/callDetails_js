function open(){function t(t,e,a){var n=Ti.Network.createHTTPClient();n.onerror=function(){Ti.API.error(this.status+" - "+this.statusText)};var l=[{SessionID:t,LanguageCode:e,VirtualNetwork:a}],h=generateXML("http://service.adminportal.vasx.com/","getCallDetails",l);n.open("POST","http://192.168.20.3:12080/vx_adminportal/AdminPortal"),n.setRequestHeader("Content-Type","text/xml"),n.setRequestHeader("Content-Length",h.length),n.setRequestHeader("SOAPAction",'"getCallDetails"'),n.send(h),n.onload=function(){var t=n.responseXML.documentElement.getElementsByTagName("getCallDetailsResponse");if(t&&t.length>0){Ti.API.info(t.item(0).text);var e=Ti.XML.parseString(t.item(0).text),a=[{title:e.getElementsByTagName("Message").item(0).text}];i.setVisible(!1),o.setData(a),c.add(s),c.add(o),r.add(c)}}}function e(){r.open()}function a(){r.close()}var r=Ti.UI.createWindow({backgroundColor:"pink",left:"51%",backgroundImage:Ti.Filesystem.resourcesDirectory+"/images/common/background.png"});Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),theme("white"),r.setBackgroundColor(Ti.App.backgroundColor),t(Ti.App.Session,Ti.App.Language,1);var n;n="iPhone OS"===Ti.Platform.name?Ti.UI.iPhone.ActivityIndicatorStyle.DARK:Ti.UI.ActivityIndicatorStyle.DARK;var i=Ti.UI.createActivityIndicator({color:Ti.App.foregroundColor,font:{fontFamily:"Helvetica Neue",fontSize:26,fontWeight:"bold"},message:"Loading...",style:n,height:Ti.UI.SIZE,width:Ti.UI.SIZE,visible:!1}),o=Ti.UI.createTableView({backgroundColor:"transparent",scrollable:!0,allowsSelection:!1,separatorColor:Ti.App.backgroundColor}),s=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,opacity:.4}),c=Ti.UI.createView({backgroundColor:"transparent",top:"3%",bottom:"3%",left:"3%",right:"3%",borderRadius:10});return phoneSwipeWindow(r),closeMenu(r),r.add(i),r.openWin=e,r.close=a,r}exports.open=open;