function open(){function t(t,e,a){var n=Ti.Network.createHTTPClient();n.onerror=function(){Ti.API.error(this.status+" - "+this.statusText)};var c=[{SessionID:t,LanguageCode:e,VirtualNetwork:a}],h=generateXML("http://service.adminportal.vasx.com/","getAgingData",c);n.open("POST","http://192.168.20.3:12080/vx_adminportal/AdminPortal"),n.setRequestHeader("Content-Type","text/xml"),n.setRequestHeader("Content-Length",h.length),n.setRequestHeader("SOAPAction",'"getAgingData"'),n.send(h),n.onload=function(){var t=n.responseXML.documentElement.getElementsByTagName("getAgingDataResponse");if(Ti.API.info(t.item(0).text),t&&t.length>0){var e=[],a=[],c=Ti.XML.parseString(t.item(0).text);c.getElementsByTagName("Message");var h=c.getElementsByTagName("CurrentBalance"),l=c.getElementsByTagName("Outstanding30Days"),u=c.getElementsByTagName("Outstanding60Days"),d=c.getElementsByTagName("Outstanding90Days"),p=c.getElementsByTagName("Outstanding120Days"),f=c.getElementsByTagName("UnbilledUsage"),g=c.getElementsByTagName("UnbilledSubs");e.push(h.item(0).text),a.push("Current Balance: "),e.push(l.item(0).text),a.push("Outstanding 30 days: "),e.push(u.item(0).text),a.push("Outstanding 60 days: "),e.push(d.item(0).text),a.push("Outstanding 90 days: "),e.push(p.item(0).text),a.push("Outstanding 120 days: "),e.push(f.item(0).text),a.push("Unbilled Usage: "),e.push(g.item(0).text),a.push("Unbilled Subscribers: ");for(var m=[],v=0;e.length>v;v++){var y=Ti.UI.createTableViewRow({selectedBackgroundColor:"transparent",height:40}),_=Ti.UI.createLabel({text:a[v],color:"black",font:{fontSize:"16pt",fontWeight:"bold"},left:"2%",right:"2%"}),b=Ti.UI.createLabel({text:"$"+e[v],font:{fontSize:"19pt",fontWeight:"bold"},right:"2%",color:"white"});y.add(_),y.add(b),m.push(y)}var R=Ti.UI.createTableView({backgroundColor:"transparent",data:m,separatorColor:Ti.App.backgroundColor});o.add(i),o.add(R),r.add(o),s.setVisible(!1)}}}function e(){r.open()}function a(){r.close()}var r=Ti.UI.createWindow({backgroundColor:"pink",left:"90%",backgroundImage:Ti.Filesystem.resourcesDirectory+"/images/common/background.png"});Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),theme("white"),r.setBackgroundColor(Ti.App.backgroundColor);var n,i=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,opacity:.4}),o=Ti.UI.createView({backgroundColor:"transparent",top:"3%",bottom:"3%",right:"3%",left:"3%",borderRadius:10});n="iPhone OS"===Ti.Platform.name?Ti.UI.iPhone.ActivityIndicatorStyle.DARK:Ti.UI.ActivityIndicatorStyle.DARK;var s=Ti.UI.createActivityIndicator({color:Ti.App.foregroundColor,font:{fontFamily:"Helvetica Neue",fontSize:26,fontWeight:"bold"},message:"Loading...",style:n,height:Ti.UI.SIZE,width:Ti.UI.SIZE,visible:!1});return s.setVisible(!0),t(Ti.App.Session,Ti.App.Language,1),r.add(s),tableSwipeWindow(r),closeMenu(r),r.openWin=e,r.close=a,r}exports.open=open;