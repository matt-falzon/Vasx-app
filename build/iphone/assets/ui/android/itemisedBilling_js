function open(){function t(t,e,a,r){var n=Ti.Network.createHTTPClient();n.onerror=function(){Ti.API.error(this.status+" - "+this.statusText)};var i=[{SessionID:t,LanguageCode:e,BillRunID:a,VirtualNetwork:r}],o=generateXML("http://service.adminportal.vasx.com/","getItemisedBilling",i);n.open("POST","http://192.168.20.3:12080/vx_adminportal/AdminPortal"),n.setRequestHeader("Content-Type","text/xml"),n.setRequestHeader("Content-Length",o.length),n.setRequestHeader("SOAPAction",'"getItemisedBilling"'),n.send(o),n.onload=function(){var t=n.responseXML.documentElement.getElementsByTagName("getItemisedBillingResponse");if(t&&t.length>0){var e=Ti.XML.parseString(t.item(0).text);alert(e.getElementsByTagName("Message").item(0).text)}}}function e(){r.open()}function a(){r.close()}var r=Ti.UI.createWindow({backgroundColor:"pink",left:"90%"});Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),theme("blue"),r.setBackgroundColor(Ti.App.backgroundColor);var n=Ti.UI.createView({layout:"vertical",backgroundColor:Ti.App.foregroundColor,top:"3%",height:"20%",left:"3%",right:"3%",borderRadius:10}),i=Ti.UI.createTextField({top:"10%",bottom:"10%",width:"90%",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER,hintText:"Enter your bundle code",backgroundColor:"white",font:{fontSize:"24sp",fontWeight:"bold"},color:Ti.App.backgroundColor,autocorrect:!1}),o=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,height:"18%",left:"3%",right:"3%",bottom:"3%",borderRadius:10}),s=Ti.UI.createView({top:"26%",bottom:"24%",left:"3%",right:"3%",backgroundColor:Ti.App.foregroundColor,borderRadius:10}),c=Ti.UI.createButton({title:"get bundle",top:"20%"});return c.addEventListener("click",function(){t(r.id,Ti.App.Language,i.getValue(),1)}),r.add(n),r.add(o),o.add(c),n.add(i),r.add(s),tableSwipeWindow(r),closeMenu(r),r.openWin=e,r.close=a,r}exports.open=open;