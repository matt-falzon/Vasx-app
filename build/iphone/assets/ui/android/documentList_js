function open(){function t(t,e,a,n){var l=Ti.Network.createHTTPClient();l.onerror=function(){Ti.API.error(this.status+" - "+this.statusText)};var h=[{SessionID:t,LanguageCode:e,ForMSISDN:a,VirtualNetwork:n}],u=generateXML("http://service.adminportal.vasx.com/","getDocumentList",h);l.open("POST","http://192.168.20.3:12080/vx_adminportal/AdminPortal"),l.setRequestHeader("Content-Type","text/xml"),l.setRequestHeader("Content-Length",u.length),l.setRequestHeader("SOAPAction",'"getDocumentList"'),l.send(u),l.onload=function(){var t=l.responseXML.documentElement.getElementsByTagName("getDocumentListResponse");if(t&&t.length>0){Ti.API.info(t.item(0).text);var e=Ti.XML.parseString(t.item(0).text);Ti.API.info(e.getElementsByTagName("Message").item(0).text);var a=[{title:e.getElementsByTagName("Message").item(0).text}];o.setData(a),c.add(s),c.add(o),r.add(c),i.setVisible(!1)}}}function e(){r.open()}function a(){r.close()}var r=Ti.UI.createWindow({backgroundColor:"pink",left:"90%",backgroundImage:Ti.Filesystem.resourcesDirectory+"/images/common/background.png"});Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),theme("white"),r.backgroundColor=Ti.App.backgroundColor;var n;n="iPhone OS"===Ti.Platform.name?Ti.UI.iPhone.ActivityIndicatorStyle.DARK:Ti.UI.ActivityIndicatorStyle.DARK;var i=Ti.UI.createActivityIndicator({color:Ti.App.foregroundColor,font:{fontFamily:"Helvetica Neue",fontSize:26,fontWeight:"bold"},message:"Loading...",style:n,height:Ti.UI.SIZE,width:Ti.UI.SIZE,visible:!1});r.addEventListener("focus",function(){i.setVisible(!0)});var o=Ti.UI.createTableView({backgroundColor:"transparent"});t(Ti.App.Session,Ti.App.Language,Ti.App.msisdn,1);var s=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,opacity:.4}),c=Ti.UI.createView({backgroundColor:"transparent",top:"3%",bottom:"3%",right:"3%",left:"3%",borderRadius:10});return o.addEventListener("click",function(t){if(t.rowData.url)var e=Ti.UI.createWindow({backgroundColor:"#fff",title:t.rowData.title,url:t.rowData.url});Ti.UI.currentTab.open(e)}),r.add(i),tableSwipeWindow(r),closeMenu(r),r.openWin=e,r.close=a,r}exports.open=open;