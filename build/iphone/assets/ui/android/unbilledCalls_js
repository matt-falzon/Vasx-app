function open(){function e(){a.open()}function t(){a.close()}var a=Ti.UI.createWindow({backgroundColor:"pink",left:"90%",backgroundImage:Ti.Filesystem.resourcesDirectory+"/images/common/background.png"});Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),theme("white"),a.setBackgroundColor(Ti.App.backgroundColor);for(var r=[],i=1;25>i;i++){var n=Ti.UI.createTableViewRow({selectedBackgroundColor:"transparent",title:"Description",height:40}),o=Ti.UI.createLabel({text:"$"+Math.floor(100*Math.random())+"."+Math.floor(99*Math.random()),font:{fontSize:24,fontWeight:"bold"},right:15,color:"white"});n.add(o),r.push(n)}var s=Ti.UI.createTableView({data:r,backgroundColor:"transparent",scrollable:!0,allowsSelection:!1,separatorColor:Ti.App.backgroundColor}),l=Ti.UI.createView({backgroundColor:"white",opacity:.4}),c=Ti.UI.createView({backgroundColor:"transparent",top:"3%",bottom:"3%",left:"3%",right:"3%",borderRadius:10});return c.add(l),a.add(c),c.add(s),tableSwipeWindow(a),closeMenu(a),a.openWin=e,a.close=t,a}exports.open=open;