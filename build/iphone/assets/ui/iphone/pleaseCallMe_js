function open(){function e(){a.open()}function t(){a.close()}var a=Ti.UI.createWindow({backgroundColor:"pink",left:"51%"});Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),theme("white"),a.backgroundColor=Ti.App.backgroundColor;var r=Ti.UI.createScrollView({backgroundColor:Ti.App.backgroundColor,top:0,horizontalBounce:!1,horizontalWrap:!1}),i=Ti.UI.createView({height:"100.5%",backgroundImage:Ti.Filesystem.resourcesDirectory+"/images/common/background.png"}),o=Ti.UI.createView({height:"70%",top:"-70%",backgroundColor:Ti.UI.backgroundColor}),n=Ti.UI.createActivityIndicator({color:"white",font:{fontFamily:"Helvetica Neue",fontSize:26,fontWeight:"bold"},message:"Updating...",style:Ti.UI.iPhone.ActivityIndicatorStyle.DARK,bottom:10});n.show();var s=Ti.UI.createView({height:Ti.UI.SIZE,layout:"vertical",backgroundColor:"transparent",top:"3%"}),l=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,opacity:.4});headerViewWrapper=Ti.UI.createView({backgroundColor:"transparent",height:"51.5%",left:"3%",right:"3%",borderRadius:10});var c=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,opacity:.4}),d=Ti.UI.createView({backgroundColor:"transparent",top:"5%",left:"3%",right:"3%",bottom:"5%",borderRadius:10,opacity:.4}),u=Ti.UI.createTextField({hintText:"Enter phone number",color:"black",top:"13%",width:"80%",height:"30%",font:{fontSize:24,fontWeight:"bold"},backgroundColor:"white",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER}),h=Ti.UI.createButton({title:"Contacts",top:"65%",left:"10%",width:"35%",bottom:"10%"}),p=Ti.UI.createButton({title:"Call Me",top:"65%",right:"10%",width:"35%",bottom:"10%"}),g=Ti.UI.createTableView({data:getContacts(),backgroundColor:"transparent",separatorColor:Ti.App.backgroundColor,color:"black"});return u.addEventListener("click",function(){u.blur(),u.keyboardType=Ti.UI.KEYBOARD_PHONE_PAD,u.focus()}),h.addEventListener("click",function(){u.blur()}),g.addEventListener("click",function(e){e.index,e.section,e.row,e.rowData;var t=e.rowData.msisdn;getContacts(),Ti.API.info(t),u.setValue(t)}),p.addEventListener("click",function(){""==u.getValue()?alert("Select a phone number first"):alert("Message sent!")}),phoneSwipeWindow(a),closeMenu(a),headerViewWrapper.add(u),i.add(s),s.add(headerViewWrapper),s.add(d),headerViewWrapper.add(l),d.add(c),headerViewWrapper.add(p),headerViewWrapper.add(h),d.add(g),a.add(r),r.add(i),r.add(o),o.add(n),a.openWin=e,a.close=t,a}exports.open=open;