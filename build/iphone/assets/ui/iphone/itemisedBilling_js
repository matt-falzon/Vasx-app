function open(){function e(e,t,a,r){var i=Ti.Network.createHTTPClient();i.onerror=function(){Ti.API.error(this.status+" - "+this.statusText)};var o=[{SessionID:e,LanguageCode:t,BillRunID:a,VirtualNetwork:r}],n=generateXML("http://service.adminportal.vasx.com/","getItemisedBilling",o);i.open("POST","http://192.168.20.3:12080/vx_adminportal/AdminPortal"),i.setRequestHeader("Content-Type","text/xml"),i.setRequestHeader("Content-Length",n.length),i.setRequestHeader("SOAPAction",'"getItemisedBilling"'),i.send(n),i.onload=function(){var e=i.responseXML.documentElement.getElementsByTagName("getItemisedBillingResponse");if(e&&e.length>0){var t=Ti.XML.parseString(e.item(0).text);alert(t.getElementsByTagName("Message").item(0).text)}}}function t(){r.open()}function a(){r.close()}var r=Ti.UI.createWindow({backgroundColor:"pink",left:"90%"});Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),theme("blue"),r.setBackgroundColor(Ti.App.backgroundColor);var i=Ti.UI.createView({layout:"vertical",backgroundColor:Ti.App.foregroundColor,top:"3%",height:"20%",left:"3%",right:"3%",borderRadius:10}),o=Ti.UI.createTextField({top:"10%",bottom:"10%",width:"90%",textAlign:Ti.UI.TEXT_ALIGNMENT_CENTER,hintText:"Enter your bundle code",backgroundColor:"white",font:{fontSize:"24sp",fontWeight:"bold"},color:Ti.App.backgroundColor,autocorrect:!1}),n=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,height:"18%",left:"3%",right:"3%",bottom:"3%",borderRadius:10}),s=Ti.UI.createView({top:"26%",bottom:"24%",left:"3%",right:"3%",backgroundColor:Ti.App.foregroundColor,borderRadius:10}),l=Ti.UI.createButton({title:"get bundle",top:"20%"});return l.addEventListener("click",function(){e(r.id,Ti.App.Language,o.getValue(),1)}),r.add(i),r.add(n),n.add(l),i.add(o),r.add(s),tableSwipeWindow(r),closeMenu(r),r.openWin=t,r.close=a,r}exports.open=open;