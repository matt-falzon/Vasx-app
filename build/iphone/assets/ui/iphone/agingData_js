function open(){function e(e,t,a){var i=Ti.Network.createHTTPClient();i.onerror=function(){Ti.API.error(this.status+" - "+this.statusText)};var l=[{SessionID:e,LanguageCode:t,VirtualNetwork:a}],c=generateXML("http://service.adminportal.vasx.com/","getAgingData",l);i.open("POST","http://192.168.20.3:12080/vx_adminportal/AdminPortal"),i.setRequestHeader("Content-Type","text/xml"),i.setRequestHeader("Content-Length",c.length),i.setRequestHeader("SOAPAction",'"getAgingData"'),i.send(c),i.onload=function(){var e=i.responseXML.documentElement.getElementsByTagName("getAgingDataResponse");if(Ti.API.info(e.item(0).text),e&&e.length>0){var t=[],a=[],l=Ti.XML.parseString(e.item(0).text);l.getElementsByTagName("Message");var c=l.getElementsByTagName("CurrentBalance"),d=l.getElementsByTagName("Outstanding30Days"),u=l.getElementsByTagName("Outstanding60Days"),h=l.getElementsByTagName("Outstanding90Days"),p=l.getElementsByTagName("Outstanding120Days"),g=l.getElementsByTagName("UnbilledUsage"),m=l.getElementsByTagName("UnbilledSubs");t.push(c.item(0).text),a.push("Current Balance: "),t.push(d.item(0).text),a.push("Outstanding 30 days: "),t.push(u.item(0).text),a.push("Outstanding 60 days: "),t.push(h.item(0).text),a.push("Outstanding 90 days: "),t.push(p.item(0).text),a.push("Outstanding 120 days: "),t.push(g.item(0).text),a.push("Unbilled Usage: "),t.push(m.item(0).text),a.push("Unbilled Subscribers: ");for(var f=[],T=0;t.length>T;T++){var v=Ti.UI.createTableViewRow({selectedBackgroundColor:"transparent",height:40}),b=Ti.UI.createLabel({text:a[T],color:"black",font:{fontSize:"16pt",fontWeight:"bold"},left:"2%",right:"2%"}),y=Ti.UI.createLabel({text:"$"+t[T],font:{fontSize:"19pt",fontWeight:"bold"},right:"2%",color:"white"});v.add(b),v.add(y),f.push(v)}var w=Ti.UI.createTableView({backgroundColor:"transparent",data:f,separatorColor:Ti.App.backgroundColor});o.add(n),o.add(w),r.add(o),s.setVisible(!1)}}}function t(){r.open()}function a(){r.close()}var r=Ti.UI.createWindow({backgroundColor:"pink",left:"90%",backgroundImage:Ti.Filesystem.resourcesDirectory+"/images/common/background.png"});Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),theme("white"),r.setBackgroundColor(Ti.App.backgroundColor);var i,n=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,opacity:.4}),o=Ti.UI.createView({backgroundColor:"transparent",top:"3%",bottom:"3%",right:"3%",left:"3%",borderRadius:10});i="iPhone OS"===Ti.Platform.name?Ti.UI.iPhone.ActivityIndicatorStyle.DARK:Ti.UI.ActivityIndicatorStyle.DARK;var s=Ti.UI.createActivityIndicator({color:Ti.App.foregroundColor,font:{fontFamily:"Helvetica Neue",fontSize:26,fontWeight:"bold"},message:"Loading...",style:i,height:Ti.UI.SIZE,width:Ti.UI.SIZE,visible:!1});return s.setVisible(!0),e(Ti.App.Session,Ti.App.Language,1),r.add(s),tableSwipeWindow(r),closeMenu(r),r.openWin=t,r.close=a,r}exports.open=open;