function open(){function e(){a.open()}function t(){a.close()}var a=Ti.UI.createWindow({backgroundColor:"pink",left:"60%",backgroundImage:Ti.Filesystem.resourcesDirectory+"/images/common/background.png"});Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),theme("white"),a.backgroundColor=Ti.App.backgroundColor,a.addEventListener("focus",function(){if("en"==Ti.App.Language)var e=[{hasDetail:!0,title:"View aging data",url:"ui/iphone/agingData"},{hasDetail:!0,title:"View unbilled calls",url:"ui/iphone/unbilledCalls"},{hasDetail:!0,title:"Document List",url:"ui/iphone/documentList"},{hasDetail:!0,title:"On net bundle",url:"ui/iphone/onNetBundle"},{hasDetail:!0,title:"Itemised Billing",url:"ui/iphone/itemisedBilling"},{hasDetail:!0,title:"Balance Enquiry",url:"ui/iphone/balance"}];else if("fr"==Ti.App.Language)var e=[{hasDetail:!0,title:"Afficher vieillissement données",url:"agingData.js"},{hasDetail:!0,title:"Voir non facturés appels",url:"unbilledCalls.js"},{hasDetail:!0,title:"Liste des documents",url:"documentList.js"}];else var e=[{hasDetail:!0,title:"Ver envelhecimento dados",url:"agingData.js"},{hasDetail:!0,title:"Ver não faturada chamadas",url:"unbilledCalls.js"},{hasDetail:!0,title:"lista de documento",url:"documentList.js"}];r.setData(e)});var r=Ti.UI.createTableView({backgroundColor:"transparent",color:"black"}),i=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,opacity:.4}),n=Ti.UI.createView({backgroundColor:"transparent",top:"3%",bottom:"3%",right:"3%",left:"3%",borderRadius:10});return r.addEventListener("click",function(e){newWin=require(e.rowData.url),open=new newWin.open,open.openWin(),setTimeout(function(){},1e3)}),phoneSwipeWindow(a),closeMenu(a),a.add(n),n.add(i),n.add(r),a.openWin=e,a.close=t,a}exports.open=open;