function open(){function e(e,t,a){var i=Ti.Network.createHTTPClient();i.onerror=function(){Ti.API.error(this.status+" - "+this.statusText)};var c=[{SessionID:e,LanguageCode:t,VirtualNetwork:a}],d=generateXML("http://service.adminportal.vasx.com/","getCallDetails",c);i.open("POST","http://192.168.20.3:12080/vx_adminportal/AdminPortal"),i.setRequestHeader("Content-Type","text/xml"),i.setRequestHeader("Content-Length",d.length),i.setRequestHeader("SOAPAction",'"getCallDetails"'),i.send(d),i.onload=function(){var e=i.responseXML.documentElement.getElementsByTagName("getCallDetailsResponse");if(e&&e.length>0){Ti.API.info(e.item(0).text);var t=Ti.XML.parseString(e.item(0).text),a=[{title:t.getElementsByTagName("Message").item(0).text}];o.setVisible(!1),n.setData(a),l.add(s),l.add(n),r.add(l)}}}function t(){r.open()}function a(){r.close()}var r=Ti.UI.createWindow({backgroundColor:"pink",left:"51%",backgroundImage:Ti.Filesystem.resourcesDirectory+"/images/common/background.png"});Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/commonFunctions.js"),Ti.include(Ti.Filesystem.resourcesDirectory+"/libs/webService.js"),theme("white"),r.setBackgroundColor(Ti.App.backgroundColor),e(Ti.App.Session,Ti.App.Language,1);var i;i="iPhone OS"===Ti.Platform.name?Ti.UI.iPhone.ActivityIndicatorStyle.DARK:Ti.UI.ActivityIndicatorStyle.DARK;var o=Ti.UI.createActivityIndicator({color:Ti.App.foregroundColor,font:{fontFamily:"Helvetica Neue",fontSize:26,fontWeight:"bold"},message:"Loading...",style:i,height:Ti.UI.SIZE,width:Ti.UI.SIZE,visible:!1}),n=Ti.UI.createTableView({backgroundColor:"transparent",scrollable:!0,allowsSelection:!1,separatorColor:Ti.App.backgroundColor}),s=Ti.UI.createView({backgroundColor:Ti.App.foregroundColor,opacity:.4}),l=Ti.UI.createView({backgroundColor:"transparent",top:"3%",bottom:"3%",left:"3%",right:"3%",borderRadius:10});return phoneSwipeWindow(r),closeMenu(r),r.add(o),r.openWin=t,r.close=a,r}exports.open=open;