var Base64={_keyStr:"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=",encode:function(t){var e,a,r,i,s,n,o,h="",c=0;for(t=Base64._utf8_encode(t);t.length>c;)e=t.charCodeAt(c++),a=t.charCodeAt(c++),r=t.charCodeAt(c++),i=e>>2,s=(3&e)<<4|a>>4,n=(15&a)<<2|r>>6,o=63&r,isNaN(a)?n=o=64:isNaN(r)&&(o=64),h=h+this._keyStr.charAt(i)+this._keyStr.charAt(s)+this._keyStr.charAt(n)+this._keyStr.charAt(o);return h},decode:function(t){var e,a,r,i,s,n,o,h="",c=0;for(t=t.replace(/[^A-Za-z0-9\+\/\=]/g,"");t.length>c;)i=this._keyStr.indexOf(t.charAt(c++)),s=this._keyStr.indexOf(t.charAt(c++)),n=this._keyStr.indexOf(t.charAt(c++)),o=this._keyStr.indexOf(t.charAt(c++)),e=i<<2|s>>4,a=(15&s)<<4|n>>2,r=(3&n)<<6|o,h+=String.fromCharCode(e),64!=n&&(h+=String.fromCharCode(a)),64!=o&&(h+=String.fromCharCode(r));return h=Base64._utf8_decode(h)},_utf8_encode:function(t){t=t.replace(/\r\n/g,"\n");for(var e="",a=0;t.length>a;a++){var r=t.charCodeAt(a);128>r?e+=String.fromCharCode(r):r>127&&2048>r?(e+=String.fromCharCode(192|r>>6),e+=String.fromCharCode(128|63&r)):(e+=String.fromCharCode(224|r>>12),e+=String.fromCharCode(128|63&r>>6),e+=String.fromCharCode(128|63&r))}return e},_utf8_decode:function(t){for(var e="",a=0,r=c1=c2=0;t.length>a;)r=t.charCodeAt(a),128>r?(e+=String.fromCharCode(r),a++):r>191&&224>r?(c2=t.charCodeAt(a+1),e+=String.fromCharCode((31&r)<<6|63&c2),a+=2):(c2=t.charCodeAt(a+1),c3=t.charCodeAt(a+2),e+=String.fromCharCode((15&r)<<12|(63&c2)<<6|63&c3),a+=3);return e}};