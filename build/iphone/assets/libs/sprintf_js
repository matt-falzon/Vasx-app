function sprintf(){var t=/%%|%(\d+\$)?([-+\'#0 ]*)(\*\d+\$|\*|\d+)?(\.(\*\d+\$|\*|\d+))?([scboxXuidfegEG])/g,e=arguments,a=0,r=e[a++],i=function(t,e,a,r){a||(a=" ");var i=t.length>=e?"":Array(1+e-t.length>>>0).join(a);return r?t+i:i+t},n=function(t,e,a,r,n,s){var o=r-t.length;return o>0&&(t=a||!n?i(t,r,s,a):t.slice(0,e.length)+i("",o,"0",!0)+t.slice(e.length)),t},s=function(t,e,a,r,s,o,h){var c=t>>>0;return a=a&&c&&{2:"0b",8:"0",16:"0x"}[e]||"",t=a+i(c.toString(e),o||0,"0",!1),n(t,a,r,s,h)},o=function(t,e,a,r,i,s){return null!=r&&(t=t.slice(0,r)),n(t,"",e,a,i,s)},h=function(t,r,h,c,l,d,u){var p,f,g,m,v;if("%%"==t)return"%";for(var y=!1,_="",b=!1,R=!1,G=" ",x=h.length,w=0;h&&x>w;w++)switch(h.charAt(w)){case" ":_=" ";break;case"+":_="+";break;case"-":y=!0;break;case"'":G=h.charAt(w+1);break;case"0":b=!0;break;case"#":R=!0}if(c=c?"*"==c?+e[a++]:"*"==c.charAt(0)?+e[c.slice(1,-1)]:+c:0,0>c&&(c=-c,y=!0),!isFinite(c))throw Error("sprintf: (minimum-)width must be finite");switch(d=d?"*"==d?+e[a++]:"*"==d.charAt(0)?+e[d.slice(1,-1)]:+d:"fFeE".indexOf(u)>-1?6:"d"==u?0:void 0,v=r?e[r.slice(0,-1)]:e[a++],u){case"s":return o(v+"",y,c,d,b,G);case"c":return o(String.fromCharCode(+v),y,c,d,b);case"b":return s(v,2,R,y,c,d,b);case"o":return s(v,8,R,y,c,d,b);case"x":return s(v,16,R,y,c,d,b);case"X":return s(v,16,R,y,c,d,b).toUpperCase();case"u":return s(v,10,R,y,c,d,b);case"i":case"d":return p=parseInt(+v),f=0>p?"-":_,v=f+i(Math.abs(p)+"",d,"0",!1),n(v,f,y,c,b);case"e":case"E":case"f":case"F":case"g":case"G":return p=+v,f=0>p?"-":_,g=["toExponential","toFixed","toPrecision"]["efg".indexOf(u.toLowerCase())],m=["toString","toUpperCase"]["eEfFgG".indexOf(u)%2],v=f+Math.abs(p)[g](d),n(v,f,y,c,b)[m]();default:return t}};return r.replace(t,h)}